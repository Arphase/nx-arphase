{
  "npmScope": "ivt",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "innovatech-ui": {
      "tags": ["front"]
    },
    "innovatech-ui-e2e": {
      "tags": ["test"],
      "implicitDependencies": ["innovatech-ui"]
    },
    "innovatech-api": {
      "tags": ["back"]
    },
    "data": {
      "tags": ["common"]
    },
    "utils": {
      "tags": ["common"]
    },
    "auth": {
      "tags": ["front"]
    },
    "dashboard": {
      "tags": ["front"]
    },
    "ui": {
      "tags": ["front"]
    },
    "guarantees": {
      "tags": ["front"]
    },
    "spa": {
      "tags": ["front"]
    },
    "state": {
      "tags": ["front"]
    }
  }
}
