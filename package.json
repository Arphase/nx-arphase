    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "analyze-bundle:innovatech": "webpack-bundle-analyzer ./dist/apps/innovatech/app/stats.json",
    "analyze-bundle:valmira": "webpack-bundle-analyzer ./dist/apps/valmira/admin/stats.json",
    "help": "nx help",
    "innovatech:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register apps/innovatech/api/src/db/scripts/seed",
    "musicr:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register apps/musicr/api/src/db/scripts/seed",
    "valmira:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register apps/valmira/api/src/db/scripts/seed",
    "typeorm": "ts-node  --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/innovatech/api/src/db/config/ormconfig.migrations.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "api:prod": "nx build innovatech-api --prod && node dist/apps/innovatech/api/main.js",
    "dev:ssr": "nx run musicr-store:serve-ssr",
    "serve:ssr": "node dist/musicr-store/server/main.js",
    "build:ssr": "nx build --prod && ng run musicr-store:server:production",
    "prerender": "nx run musicr-store:prerender"
  },
