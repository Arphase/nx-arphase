    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx serve",
    "start:api": "nx serve innovatech-api",
    "build": "nx build",
    "build:stats": "nx build --prod --stats-json",
    "test": "nx test",
