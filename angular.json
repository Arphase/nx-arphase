{
  "version": 1,
  "projects": {
    "ivt-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "less"
        }
      },
      "root": "apps/ivt-app/",
      "sourceRoot": "apps/ivt-app/src",
      "prefix": "ivt",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ivt-app",
            "index": "apps/ivt-app/src/index.html",
            "main": "apps/ivt-app/src/main.ts",
            "polyfills": "apps/ivt-app/src/polyfills.ts",
            "tsConfig": "apps/ivt-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/ivt-app/src/favicon.ico",
              "apps/ivt-app/src/assets",
              "apps/ivt-app/src/manifest.webmanifest"
            ],
            "styles": [
              "apps/ivt-app/src/styles.less",
              {
                "input": "libs/ui/u-ui/src/styles/default.less",
                "bundleName": "default",
                "inject": false
              },
              {
                "input": "libs/ui/u-ui/src/styles/dark.less",
                "bundleName": "dark",
                "inject": false
              }
            ],
            "allowedCommonJsDependencies": ["file-saver"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui/u-ui/src/styles"]
            }
          },
          "configurations": {
            "stage": {
              "fileReplacements": [
                {
                  "replace": "apps/ivt-app/src/environments/environment.ts",
                  "with": "apps/ivt-app/src/environments/environment.stage.ts"
                }
              ],
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "vendor": true,
                "hidden": false
              },
              "optimization": true,
              "outputHashing": "all",
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/ivt-app/ngsw-config.json"
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ivt-app/src/environments/environment.ts",
                  "with": "apps/ivt-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/ivt-app/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ivt-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ivt-app:build:production"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ivt-app:build"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ivt-app/jest.config.js"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ivt-app/**/*.ts"]
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "ivt-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ivt-app:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "ivt-app:ionic-cordova-build",
            "devServerTarget": "ivt-app:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "ivt-app:ionic-cordova-build:production",
              "devServerTarget": "ivt-app:serve:production"
            }
          }
        }
      }
    },
