    "typeorm:innovatech:migrate": "npm run typeorm:innovatech migration:generate -- -n",
    "typeorm:innovatech:run": "npm run typeorm:innovatech migration:run",
    "typeorm:valmira": "ts-node  --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/valmira/api/src/db/config/ormconfig.migrations.ts",
