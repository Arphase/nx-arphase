{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "declaration": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "commonjs",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "paths": {
      "@innovatech/app/*": [
        "apps/innovatech/app/src/app/*"
      ],
      "@innovatech/app/env/*": [
        "apps/innovatech/app/src/environments/*"
      ],
      "@innovatech/api/*": [
        "apps/innovatech/api/src/app/*"
      ],
      "@innovatech.api/env/*": [
        "apps/innovatech/api/src/environments/*"
      ],
      "@ivt/a-auth": [
        "libs/api/a-auth/src/index.ts"
      ],
      "@ivt/a-groups": [
        "libs/api/a-groups/src/index.ts"
      ],
      "@ivt/a-companies": [
        "libs/api/a-companies/src/index.ts"
      ],
      "@ivt/a-guarantees": [
        "libs/api/a-guarantees/src/index.ts"
      ],
      "@ivt/a-localities": [
        "libs/api/a-localities/src/index.ts"
      ],
      "@ivt/a-payment-orders": [
        "libs/api/a-payment-orders/src/index.ts"
      ],
      "@ivt/a-products": [
        "libs/api/a-products/src/index.ts"
      ],
      "@ivt/a-revision-requests": [
        "libs/api/a-revision-requests/src/index.ts"
      ],
      "@ivt/a-revisions": [
        "libs/api/a-revisions/src/index.ts"
      ],
      "@ivt/a-state": [
        "libs/api/a-state/src/index.ts"
      ],
      "@ivt/a-users": [
        "libs/api/a-users/src/index.ts"
      ],
      "@ivt/a-vehicles": [
        "libs/api/a-vehicles/src/index.ts"
      ],
      "@ivt/c-utils": [
        "libs/common/c-utils/src/index.ts"
      ],
      "@innovatech/common/domain": [
        "libs/innovatech/common/domain/src/index.ts"
      ],
      "@ivt/u-ui": [
        "libs/ui/u-ui/src/index.ts"
      ],
      "@innovatech/ui/guarantees": [
        "libs/innovatech/ui/guarantees/src/index.ts"
      ],
      "@ivt/u-auth": [
        "libs/ui/u-auth/src/index.ts"
      ],
      "@innovatech/ui/dashboard": [
        "libs/innovatech/ui/dashboard/src/index.ts"
      ],
      "@innovatech/ui/spa": [
        "libs/innovatech/ui/spa/src/index.ts"
      ],
      "@ivt/u-state": [
        "libs/ui/u-state/src/index.ts"
      ],
      "@ivt/u-groups": [
        "libs/ui/u-groups/src/index.ts"
      ],
      "@ivt/u-products": [
        "libs/ui/u-products/src/index.ts"
      ],
      "@ivt/u-revision-requests": [
        "libs/ui/u-revision-requests/src/index.ts"
      ],
      "@ivt/u-revisions": [
        "libs/ui/u-revisions/src/index.ts"
      ],
      "@ivt/u-tests": [
        "libs/ui/u-tests/src/index.ts"
