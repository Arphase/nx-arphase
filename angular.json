{
  "version": 1,
  "projects": {
    "innovatech-ui": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/innovatech-ui",
      "sourceRoot": "apps/innovatech-ui/src",
      "prefix": "innovatech",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/innovatech-ui",
            "index": "apps/innovatech-ui/src/index.html",
            "main": "apps/innovatech-ui/src/main.ts",
            "polyfills": "apps/innovatech-ui/src/polyfills.ts",
            "tsConfig": "apps/innovatech-ui/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/innovatech-ui/src/favicon.ico", "apps/innovatech-ui/src/assets"],
            "styles": ["apps/innovatech-ui/src/styles.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": ["file-saver"]
          },
          "configurations": {
            "qa": {
              "fileReplacements": [
                {
                  "replace": "apps/innovatech-ui/src/environments/environment.ts",
                  "with": "apps/innovatech-ui/src/environments/environment.qa.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/innovatech-ui/src/environments/environment.ts",
                  "with": "apps/innovatech-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "innovatech-ui:build",
            "proxyConfig": "apps/innovatech-ui/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "innovatech-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "innovatech-ui:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/innovatech-ui/tsconfig.app.json", "apps/innovatech-ui/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/innovatech-ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/innovatech-ui/jest.config.js",
            "tsConfig": "apps/innovatech-ui/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/innovatech-ui/src/test-setup.ts"
          }
        }
      }
    },
    "innovatech-ui-e2e": {
