{
  "name": "nx-arphase",
  "version": "4.2.3",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx serve",
    "start:api": "nx serve innovatech-api",
    "build": "nx build",
    "build:stats": "nx build --prod --stats-json",
    "test": "dotenv -e .env.test",
    "lint": "nx workspace-lint && ng lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test:staging": "nx affected:test --ci --runInBand -- --base=remotes/origin/staging",
    "affected:test:master": "nx affected:test --ci --runInBand -- --base=remotes/origin/master",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "analyze-bundle:innovatech": "npx webpack-bundle-analyzer ./dist/apps/innovatech/app/stats.json",
    "analyze-bundle:musicr": "npx webpack-bundle-analyzer ./dist/apps/musicr/browser/stats.json",
    "analyze-bundle:valmira": "npx webpack-bundle-analyzer ./dist/apps/valmira/admin/stats.json",
    "help": "nx help",
    "innovatech:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register libs/innovatech/api/db/src/lib/scripts/seed/seed",
    "valmira:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register apps/valmira/api/src/db/scripts/seed",
    "musicr:seed": "dotenv -e .env -- ts-node --project tsconfig.base.json -r tsconfig-paths/register libs/musicr/api/db/src/lib/scripts/seed",
    "typeorm:innovatech": "ts-node --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d libs/innovatech/api/db/src/lib/config/ormconfig.ts",
    "typeorm:innovatech:migrate": "npm run typeorm:innovatech migration:generate --",
    "typeorm:innovatech:run": "npm run typeorm:innovatech migration:run",
    "typeorm:musicr": "dotenv -e .env -- ts-node --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d libs/musicr/api/db/src/lib/config/ormconfig.ts",
    "typeorm:musicr:migrate": "npm run typeorm:musicr migration:generate --",
    "typeorm:musicr:run": "npm run typeorm:musicr migration:run",
    "typeorm:valmira": "ts-node --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/valmira/api/src/db/config/ormconfig.ts",
    "typeorm:valmira:migrate": "npm run typeorm:valmira migration:generate --",
    "typeorm:valmira:run": "npm run typeorm:valmira migration:run",
    "build:api:valmira:prod": "ts-node ./apps/valmira/app/src/environments/set-environment.ts && cp -a ./apps/valmira/api/src/assets ./dist/apps/valmira/server",
    "build:api:musicr:prod": "ts-node --project ./apps/musicr/store/tsconfig.build.json ./apps/musicr/store/src/environments/set-environment.ts && cp -a ./apps/musicr/api/src/assets ./dist/apps/musicr/server",
    "api:innovatech:prod": "chmod +rwx dist && node dist/apps/innovatech/api/main.js",
    "api:valmira:prod": "npm run build:api:valmira:prod && chmod +rwx dist && node dist/apps/valmira/server/main.js",
    "api:musicr:prod": "npm run build:api:musicr:prod && chmod +rwx dist && node dist/apps/musicr/server/main.js",
    "e2e:watch": "dotenv -e .env.test -- npx nx serve innovatech-api & npx wait-on http://localhost:3333 && dotenv -e .env.test -- npx nx affected:e2e --watch"
  },
  "private": true,
