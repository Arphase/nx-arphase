  "name": "ivt",
  "version": "3.0.4",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx serve",
    "start:api": "nx serve innovatech-api",
    "build": "nx build",
    "build:stats": "nx build --prod --stats-json",
    "test": "nx test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test:master": "nx affected:test --ci --runInBand -- --base=remotes/origin/master",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "analyze-bundle": "webpack-bundle-analyzer ./dist/apps/innovatech/app/stats.json",
    "help": "nx help",
    "innovatech:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register apps/innovatech/api/src/db/scripts/seed",
    "musicr:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register apps/musicr/api/src/db/scripts/seed",
    "typeorm": "ts-node  --project tsconfig.base.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config apps/innovatech/api/src/db/config/ormconfig.migrations.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "api:prod": "nx build innovatech-api --prod && node dist/apps/innovatech/api/main.js",
    "prepare.ionic.innovatech-app.ios": "cd apps/innovatech/app && npx rimraf -- ios && npm run cap.add.ios && npm run cap.res.ios",
    "prepare.ionic.innovatech-app.android": "cd apps/innovatech/app && npx rimraf -- android && npm run cap.add.android && npm run cap.res.android",
    "open.ionic.innovatech-app.ios": "cd apps/innovatech/app && npm run cap.ios",
    "open.ionic.innovatech-app.android": "cd apps/innovatech/app && npm run cap.android",
    "sync.ionic.innovatech-app": "cd apps/innovatech/app && npm run cap.copy",
    "clean.ionic.innovatech-app": "cd apps/innovatech/app && npx rimraf -- hooks node_modules platforms www plugins package-lock.json && npm i && rimraf -- package-lock.json"
  },
